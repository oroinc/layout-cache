{
    "name": "oro/layout-cache",
    "type": "symfony-bundle",
    "keywords": ["Oro", "Layout", "Cache"],
    "homepage": "https://github.com/oroinc/layout-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.oroinc.com"
        }
    ],
    "require": {
        "oro/commerce": "4.2.*",
        "oro/customer-portal": "4.2.*"
    },
    "repositories": {
        "composer": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        }
    },
    "autoload": {
        "psr-4": {
            "Oro\\Bundle\\LayoutCacheBundle\\": "src/Oro/Bundle/LayoutCacheBundle"
        },
        "exclude-from-classmap": [
            "src/Oro/Bundle/LayoutCacheBundle/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.2-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "9.1.*"
    }
}
